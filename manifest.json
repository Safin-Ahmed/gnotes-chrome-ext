{
  "manifest_version": 2,
  "name": "GNotes",
  "version": "1.0",
  "description": "Jot down any words you want on the go!",
  "icons": {
    "128": "icon128.png",
    "48": "icon32.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
